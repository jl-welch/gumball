{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "browser": true
  },
  "rules": {
    "no-multi-assign": 0,
    "no-underscore-dangle": 0,
    "no-plusplus": 0,
    "import/prefer-default-export": 0,
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  }
}
